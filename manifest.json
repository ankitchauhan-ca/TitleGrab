{
  "manifest_version": 3,
  "name": "TabGrabber",
  "version": "1.0",
  "description": "A Chrome extension that gets the title of the current tab",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
